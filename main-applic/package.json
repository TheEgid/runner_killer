{
    "$schema": "http://json.schemastore.org/package",
    "name": "noname",
    "private": true,
    "license": "UNLICENSED",
    "packageManager": "pnpm@10.17.1",
    "scripts": {
        "upgrade_packages": "pnpm up --latest",
        "build": "pnpm select-schema && dotenvx run -f ../.env -- next build",
        "start": "dotenvx run -f ../.env -- next start -p 3006",
        "frmt": "eslint . --ignore-pattern \"build/*\" --fix",
        "prisma-generate": "pnpm prisma generate",
        "prisma-push": "pnpm prisma db push",
        "prisma-studio": "pnpm prisma studio",
        "select-schema": "pnpm tsx ./prisma/select-schema.ts",
        "serve": "pnpm build && pnpm start",
        "stylelint": "stylelint \"**/*.scss\" --fix",
        "start_dev": "pnpm select-schema && dotenvx run -f ../.env -- next dev --port 3006 --turbo",
        "prefect_start": "dotenvx run -f ../.env -- pnpm tsx scripts/startPrefect.ts",
        "dev_all": "concurrently \"pnpm prefect_start\" \"pnpm start_dev\" "
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.51.0",
        "@mantine/colors-generator": "^8.3.2",
        "@mantine/core": "^8.3.2",
        "@mantine/dates": "^8.3.2",
        "@mantine/form": "^8.3.2",
        "@mantine/hooks": "^8.3.2",
        "@mantine/notifications": "^8.3.2",
        "@prisma/client": "6.16.3",
        "argon2": "^0.44.0",
        "cookies-next": "^6.1.0",
        "dayjs": "^1.11.18",
        "effector": "^23.4.4",
        "effector-react": "^23.3.0",
        "effector-storage": "^7.1.0",
        "ioredis": "^5.8.0",
        "jest": "^30.2.0",
        "jsonwebtoken": "^9.0.2",
        "ky": "^1.11.0",
        "next": "^15.5.4",
        "nextjs-progressbar": "^0.0.16",
        "node-cache": "^5.1.2",
        "prisma": "^6.16.3",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icons": "^5.5.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.4.0",
        "@eslint/js": "9.36.0",
        "@next/eslint-plugin-next": "^15.5.4",
        "@stylistic/eslint-plugin": "^5.4.0",
        "@stylistic/stylelint-config": "^3.0.1",
        "@stylistic/stylelint-plugin": "^4.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "24.6.0",
        "@types/react": "^19.1.16",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "concurrently": "^9.2.1",
        "eslint": "9.36.0",
        "eslint-config-next": "15.5.4",
        "eslint-plugin-effector": "^0.16.0",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "reset-css": "^5.0.2",
        "sass": "^1.93.2",
        "stylelint": "^16.24.0",
        "stylelint-config-standard-scss": "^16.0.0",
        "stylelint-order": "^7.0.0",
        "stylelint-order-config-standard": "^0.1.3",
        "stylelint-scss": "^6.12.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "typescript": "5.9.2",
        "typescript-eslint": "^8.45.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@parcel/watcher",
            "@prisma/client",
            "@prisma/engines",
            "argon2",
            "esbuild",
            "prisma",
            "sharp",
            "unrs-resolver"
        ]
    }
}
